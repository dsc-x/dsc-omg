{
  "private": true,
  "name": "dscomg",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run -s serve || npm run -s dev",
    "build": "node route-generator.js && preact build --template src/template.html --prerenderUrls src/prerender-urls.json --service-worker false --dest build",
    "serve": "preact build --template src/template.html --service-worker false && preact serve",
    "dev": "preact watch --template src/template.html",
    "lint": "eslint src"
  },
  "eslintConfig": {
    "extends": "eslint-config-synacor",
    "rules": {
      "no-unused-vars": "warn",
      "react/sort-comp": "off",
      "lines-around-comment": "off",
      "react/prefer-stateless-function": "off"
    }
  },
  "eslintIgnore": [
    "build/*"
  ],
  "devDependencies": {
    "copy-webpack-plugin": "^4.6.0",
    "eslint": "^4.5.0",
    "eslint-config-synacor": "^1.1.0",
    "if-env": "^1.0.0",
    "preact-cli": "^2.2.1",
    "preact-cli-plugin-netlify": "^1.1.0",
    "preact-cli-sw-precache": "^1.0.3",
    "webpack-bundle-analyzer": "^3.3.2",
    "xmlbuilder": "^11.0.0"
  },
  "dependencies": {
    "axios": "^0.19.2",
    "firebase": "^6.1.0",
    "hh-mm-ss": "^1.2.0",
    "idb": "^2.1.3",
    "lottie-web": "^5.4.4",
    "moment-timezone": "^0.5.31",
    "npm": "^6.9.0",
    "preact": "^8.4.2",
    "preact-compat": "^3.18.4",
    "preact-material-components": "^1.5.7",
    "preact-router": "^2.5.5",
    "react-twitter-embed": "^3.0.3",
    "xmlhttprequest": "^1.8.0"
  }
}
